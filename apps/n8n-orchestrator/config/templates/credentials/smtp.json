{
  "name": "SMTP Transport",
  "type": "smtp",
  "data": {
    "user": "={{ decodeURIComponent(new URL($env.SMTP_URL).username) }}",
    "password": "={{ decodeURIComponent(new URL($env.SMTP_URL).password) }}",
    "host": "={{ new URL($env.SMTP_URL).hostname }}",
    "port": "={{ Number(new URL($env.SMTP_URL).port || 587) }}",
    "secure": "={{ new URL($env.SMTP_URL).protocol === 'smtps:' }}"
  },
  "nodesAccess": []
}
