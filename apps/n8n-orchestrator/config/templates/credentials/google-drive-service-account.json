{
  "name": "Google Drive Service Account",
  "type": "googleDriveServiceAccount",
  "data": {
    "email": "={{ JSON.parse($env.GOOGLE_DRIVE_SERVICE_ACCOUNT_JSON).client_email }}",
    "projectId": "={{ JSON.parse($env.GOOGLE_DRIVE_SERVICE_ACCOUNT_JSON).project_id }}",
    "privateKey": "={{ JSON.parse($env.GOOGLE_DRIVE_SERVICE_ACCOUNT_JSON).private_key }}",
    "scopes": [
      "https://www.googleapis.com/auth/drive",
      "https://www.googleapis.com/auth/spreadsheets"
    ],
    "impersonate": false
  },
  "nodesAccess": []
}
